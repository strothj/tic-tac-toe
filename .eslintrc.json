{
    "env": {
        "browser": true,
        "jquery": true
    },
    "rules": {
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "constructor-super": "warn",
        "valid-typeof": "warn",
        "func-names": "off"
    },
    "extends": "eslint-config-es5"
}